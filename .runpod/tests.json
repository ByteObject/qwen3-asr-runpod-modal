{
  "tests": [
    {
      "name": "basic_english_transcription",
      "input": {
        "audio": "https://raw.githubusercontent.com/mozilla/DeepSpeech/master/data/smoke_test/LDC93S1.wav"
      },
      "timeout": 120000
    },
    {
      "name": "transcription_with_timestamps",
      "input": {
        "audio": "https://raw.githubusercontent.com/speechbrain/speechbrain/develop/tests/samples/single-mic/example1.wav",
        "return_timestamps": true
      },
      "timeout": 120000
    },
    {
      "name": "short_speech_transcription",
      "input": {
        "audio": "https://raw.githubusercontent.com/coqui-ai/TTS/dev/tests/inputs/example_1.wav"
      },
      "timeout": 120000
    },
    {
      "name": "batch_transcription",
      "input": {
        "batch": [
          {"audio": "https://raw.githubusercontent.com/mozilla/DeepSpeech/master/data/smoke_test/LDC93S1.wav"},
          {"audio": "https://raw.githubusercontent.com/coqui-ai/TTS/dev/tests/inputs/example_1.wav", "return_timestamps": true}
        ]
      },
      "timeout": 180000
    }
  ],
  "config": {
    "gpuTypeId": "NVIDIA RTX A4000",
    "gpuCount": 1,
    "allowedCudaVersions": ["12.8", "12.7", "12.6", "12.5", "12.4", "12.3", "12.2", "12.1", "12.0"]
  }
}
