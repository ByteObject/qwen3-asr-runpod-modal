{
  "tests": [
    {
      "name": "basic_english_transcription",
      "input": {
        "audio": "https://github.com/QwenLM/Qwen3-ASR/raw/main/assets/audio_en.wav"
      },
      "timeout": 120000
    },
    {
      "name": "transcription_with_timestamps",
      "input": {
        "audio": "https://github.com/QwenLM/Qwen3-ASR/raw/main/assets/audio_en.wav",
        "return_timestamps": true
      },
      "timeout": 120000
    },
    {
      "name": "chinese_transcription",
      "input": {
        "audio": "https://github.com/QwenLM/Qwen3-ASR/raw/main/assets/audio_cn.wav"
      },
      "timeout": 120000
    },
    {
      "name": "batch_transcription",
      "input": {
        "batch": [
          {"audio": "https://github.com/QwenLM/Qwen3-ASR/raw/main/assets/audio_en.wav"},
          {"audio": "https://github.com/QwenLM/Qwen3-ASR/raw/main/assets/audio_cn.wav", "return_timestamps": true}
        ]
      },
      "timeout": 180000
    }
  ],
  "config": {
    "gpuTypeId": "NVIDIA RTX A4000",
    "gpuCount": 1,
    "allowedCudaVersions": ["12.8", "12.7", "12.6", "12.5", "12.4", "12.3", "12.2", "12.1", "12.0"]
  }
}
