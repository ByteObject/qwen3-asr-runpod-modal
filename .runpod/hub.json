{
  "title": "Qwen3-ASR",
  "description": "State-of-the-art speech recognition powered by Qwen3-ASR. Supports 30+ languages, 22 Chinese dialects, batch processing, and timestamp output.",
  "type": "serverless",
  "category": "audio",
  "iconUrl": "https://cdn.prod.website-files.com/67d20fb9f56ff2ec6a7a657d/685b40411e38197aff6351df_poddy-3.webp",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 20,
    "env": [
      {
        "key": "HF_HOME",
        "value": "/runpod-volume"
      },
      {
        "key": "MODEL_NAME",
        "value": "Qwen/Qwen3-ASR-1.7B"
      },
      {
        "key": "MAX_BATCH_SIZE",
        "value": "32"
      }
    ]
  }
}
